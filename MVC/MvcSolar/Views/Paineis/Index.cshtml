@model IEnumerable<MvcSolar.Models.Painel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProducaoPrevistaHora)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modelo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Habitacao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProducaoPrevistaHora)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modelo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Habitacao.HabitacaoID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PainelID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PainelID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PainelID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
